{
  "expo": {
    "name": "YomiHomies",
    "slug": "YomiHomies",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/YH_Logo_blue.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.cc18.yomihomies",
      "supportsTablet": true
    },
    "android": {
      "package": "com.cc18.yomihomies",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/YH_Logo_blue.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
